body {
  background-color: rgb(94, 94, 94);
  background-image: url(../images/background.jpg);
  background-repeat: repeat-x;
  text-align: center;
}

#body_margin {
  margin-top: 0px;
  margin-left: auto;
  margin-right: auto;
  margin-width: 0px;
}

#main {
  background-color: rgb(255, 255, 255);
  padding: 5px;
  margin-top: 20px;

  margin-right: auto;
  margin-left: auto;
  width: 90%;
  text-align: left;
}

#main_sub {

}

#columns {
  width: 100%;
  height: 100%;
}

#col_left {
  width: 263px;
}

#col_mid {
  width: 20px;
  background-image: url(../images/fade_right.gif);
  background-repeat: repeat-y;
}

.col_mid_block {
  height: 9px;
  font-size: 1px;
}

#col_right {

}

#logo {
  margin-top: 3px;
  margin-left: 3px;
  margin-bottom: 1px;
  margin-right: 3px;
  border: none;
}

#ad {
  background-color: rgb(241, 241, 241);
  margin-top: 2px;
  margin-left: 3px;
  margin-bottom: 1px;
  margin-right: 3px;
  border: none;
  height: 36px;
  text-align: left;
  padding-top: 5px;
  padding-bottom: 5px;
  padding-left: 30px;
  font-family: Helvetica, sans-serif;
  font-size: 14px;
  font-style: italic;
}

.dk_grey {
  background-color: rgb(150, 150, 150);
  margin-top: 2px;
  margin-left: 3px;
  margin-bottom: 1px;
  margin-right: 3px;
  border: none;
  height: 6px;
  font-size: 1px;
}

.lt_grey {
  background-color: rgb(241, 241, 241);
  margin-top: 2px;
  margin-left: 3px;
  margin-bottom: 1px;
  margin-right: 3px;
  border: none;
  height: 6px;
  font-size: 1px;
}

.lt_blue {
  background-color: rgb(142, 194, 245);
  margin-top: 2px;
  margin-left: 3px;
  margin-bottom: 1px;
  margin-right: 3px;
  border: none;
  height: 6px;
  font-size: 1px;
}

.sites {
  font-family: Tahoma, Helvetica, sans-serif;
  font-size: 10px;
  font-weight: bold;
  color: rgb(146, 146, 148);
  margin-top: 2px;
  margin-left: 3px;
  margin-bottom: 1px;
  margin-right: 3px;
  border: none;
  height: 12px;
  text-align: center;
  padding-top: 8px;
  padding-bottom: 8px;
}

.site_links {
  font-family: Tahoma, Helvetica, sans-serif;
  font-size: 10px;
  font-weight: bold;
  color: rgb(96, 96, 98);
  text-decoration: none;
}

.site_links:visited {
  color: rgb(96, 96, 98);
  text-decoration: none;
}

.site_links:hover {
  color: rgb(76, 142, 231);
  text-decoration: none;
}

.site_links:active {
  color: rgb(76, 142, 231);
  text-decoration: none;
}

.heading {
  font-family: Tahoma, Helvetica, sans-serif;
  font-size: 12px;
  font-weight: bold;
  color: rgb(96, 96, 98);
  background-color: rgb(241, 241, 241);

  margin-top: 2px;
  margin-left: 3px;
  margin-bottom: 2px;
  margin-right: 3px;

  border: none;
  height: 14px;
  text-align: left;

  padding-left: 20px;
  padding-top: 7px;
  padding-bottom: 7px;
}


.menu {
  background-color: rgb(224, 224, 224);
  margin-left: 3px;
  margin-right: 3px;
  border: none;
  height: 16px;
  text-align: left;
  padding-left: 26px;
  padding-top: 4px;
  padding-bottom: 4px;
}

.menu_links {
  font-family: Helvetica, sans-serif;
  font-size: 11px;
  color: rgb(96, 96, 98);
  text-decoration: none;
}

.menu_links:visited {
  color: rgb(96, 96, 98);
  text-decoration: none;
}

.menu_links:hover {
  color: rgb(76, 142, 231);
  text-decoration: none;
}

.menu_links:active {
  color: rgb(76, 142, 231);
  text-decoration: none;
}

.bullet {
  margin-right: 12px;
  text-align: center;
}

.dashed {
  background-color: rgb(224, 224, 224);
  background-image: url(../images/div.gif);
  background-repeat: no-repeat;
  font-size: 1px;
  height: 1px;
  margin-left: 3px;
  margin-right: 3px;
  border: none;
}

.copyright {
  font-family: Arial, Helvetica, sans-serif;
  font-size: 9px;
  color: rgb(96, 96, 98);
  margin-top: 30px;
  margin-left: 6px;
  margin-right: 6px;
}

.fade_right {
  background-image: url(../images/fade_right.gif);
  background-repeat: repeat-y;
}

p {
  font-family: Tahoma, Helvetica, sans-serif;
  font-size: 12px;
  color: rgb(96, 96, 98);
  margin-left: 10px;
  margin-right: 10px;
  padding-top: 3px;
  padding-bottom: 3px;
}

h1 {
  font-family: Helvetica, sans-serif;
  font-size: 16px;
  font-weight: bold;
  color: rgb(76, 142, 231);
}

h2 {
  font-family: Helvetica, sans-serif;
  font-size: 12px;
  font-weight: bold;
  color: rgb(76, 142, 231);
}

.file {
  font-family: Tahoma, Helvetica, sans-serif;
  font-size: 13px;
  font-weight: bold;
  color: rgb(76, 142, 231);
}

.fileblock {
  margin-right: 10px;
}

.list {
  font-family: Tahoma, Helvetica, sans-serif;
  font-size: 12px;
  color: rgb(96, 96, 98);
}

#list:visited {
  color: rgb(96, 96, 98);
  text-decoration: none;
}

#list:hover {
  color: rgb(76, 142, 231);
  text-decoration: none;
}

#list:active {
  color: rgb(76, 142, 231);
  text-decoration: none;
}


ul {
  list-style-type: none;
  padding: 0;
  margin-left: 14px;
}

li {
  background-image: url(../images/block.gif);
  background-repeat: no-repeat;
  background-position: 0 8px;
  padding-left: 10px;
}

input {
  font-family: Tahoma, Helvetica, sans-serif;
  font-size: 10px;
  font-weight: bold;
  color: rgb(146, 146, 148);
}

.radio {
  font-family: Tahoma, Helvetica, sans-serif;
  font-size: 10px;
  font-weight: bold;
  color: rgb(146, 146, 148);
}