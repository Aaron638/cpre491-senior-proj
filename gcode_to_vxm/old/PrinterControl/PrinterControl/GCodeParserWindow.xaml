        <TextBox x:Name="textBox_Copy4" HorizontalAlignment="Left" Height="24" Margin="298,200,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="64"/>
        <Label x:Name="label_Copy4" Content="Number of infill squares (x):" HorizontalAlignment="Left" Margin="142,198,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.882,0.269"/>
        <TextBox x:Name="textBox_Copy5" HorizontalAlignment="Left" Height="24" Margin="298,230,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="64"/>
        <Label x:Name="label_Copy5" Content="Number of infill squares (y):" HorizontalAlignment="Left" Margin="142,228,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.882,0.269"/>
        <TextBox x:Name="textBox_Copy9" HorizontalAlignment="Left" Height="24" Margin="298,310,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="64"/>
        <Label x:Name="label_Copy9" Content="Defect size (x, y, z):" HorizontalAlignment="Left" Margin="46,308,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.882,0.269"/>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="242,136,0,0" VerticalAlignment="Top" Width="120">
            <ComboBoxItem>Checkerboard</ComboBoxItem>
            <ComboBoxItem>Random</ComboBoxItem>
            <ComboBoxItem>None</ComboBoxItem>
        </ComboBox>
        <Label x:Name="label2" Content="Hatch direction alternation:" HorizontalAlignment="Left" Margin="88,134,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="comboBox_Copy" HorizontalAlignment="Left" Margin="242,262,0,0" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label2_Copy" Content="Infill square order:" HorizontalAlignment="Left" Margin="136,258,0,0" VerticalAlignment="Top"/>
        <CheckBox x:Name="checkBox" Content="Defect?" HorizontalAlignment="Left" Margin="300,290,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox_Copy10" HorizontalAlignment="Left" Height="24" Margin="226,310,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="64"/>
        <TextBox x:Name="textBox_Copy11" HorizontalAlignment="Left" Height="24" Margin="154,310,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="64"/>
        <TextBox x:Name="textBox_Copy8" HorizontalAlignment="Left" Height="24" Margin="298,344,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="64"/>
        <Label x:Name="label_Copy8" Content="Defect location (x, y, z):" HorizontalAlignment="Left" Margin="24,342,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.882,0.269"/>
        <TextBox x:Name="textBox_Copy12" HorizontalAlignment="Left" Height="24" Margin="226,344,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="64"/>
        <TextBox x:Name="textBox_Copy13" HorizontalAlignment="Left" Height="24" Margin="154,344,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="64"/>
        <Button x:Name="button" Content="Export gCode" HorizontalAlignment="Left" Margin="274,409,0,0" VerticalAlignment="Top" Width="88" RenderTransformOrigin="-0.169,0.417" Click="button_Click"/>
        <Button x:Name="button1" Content="Close" HorizontalAlignment="Left" Margin="194,409,0,0" VerticalAlignment="Top" Width="74"/>

    </Grid>
</Window>
